{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AzureAd": {
    "Authority": "https://zuercheram1.ciamlogin.com/",
    "Domain": "zuercheram1.onmicrosoft.com",
    "TenantId": "<<Secret goes here>>",
    "ClientId": "<<Secret goes here>>"
  },
  "AllowedHosts": "*",
  "SessionCookieLifetimeMinutes": 60,
  "ReverseProxy": {
    "Routes": {
      "projects": {
        "ClusterId": "projects",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/projects-api/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/projects-api"
          }
        ]
      },
      "identity": {
        "ClusterId": "identity",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/identity-api/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/identity-api"
          }
        ]
      },
      "webapp": {
        "ClusterId": "webapp",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "projects": {
        "Destinations": {
          "projectsDestination": {
            "Address": "https://capp-proj-serv"
          }
        }
      },
      "identity": {
        "Destinations": {
          "identityDestination": {
            "Address": "https://capp-id-serv"
          }
        }
      },
      "webapp": {
        "Destinations": {
          "webappDestination": {
            "Address": "https://web-client"
          }
        }
      },
      "LoadBalancing": {
        "projects": {
          "Policy": "RoundRobin"
        },
        "identity": {
          "Policy": "RoundRobin"
        }
      }
    }
  }
}
